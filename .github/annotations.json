{
	"problemMatcher": [
		{
			"owner": "eslint",
			"pattern": [
				{
					"regexp": "^([^\\s].*)$",
					"file": 1
				},
				{
					"regexp": "^\\s+(\\d+):(\\d+)\\s+(error|warning|info)\\s+(.*)\\s\\s+(.*)$",
					"line": 1,
					"column": 2,
					"severity": 3,
					"message": 4,
					"code": 5,
					"loop": true
				}
			]
		},
		{
			"owner": "typescript",
			"pattern": [
				{
					"regexp": "^([^\\s].*):(\\d+):(\\d+)\\s+-\\s+(error|warning|info)\\s(TS\\d+):\\s(.*)$",
					"file": 1,
					"line": 2,
					"column": 3,
					"severity": 4,
					"code": 5,
					"message": 6
				}
			]
		}
	]
}