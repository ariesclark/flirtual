{
	"problemMatcher": [
		{
			"owner": "typescript",
			"fileLocation": [
				"relative",
				"${GITHUB_WORKSPACE}/apps/frontend"
			],
			"pattern": [
				{
					"regexp": "^([^\\s].*)\\((\\d+),(\\d+)\\):\\s+(error|warning|info)\\s(TS\\d+):\\s(.*)$",
					"file": 1,
					"line": 2,
					"column": 3,
					"severity": 4,
					"code": 5,
					"message": 6
				}
			]
		}
	]
}